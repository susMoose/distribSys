package cs455.overlay.wireformats;

public class Message {
	private int messageNumber;
	
	public int getMessageNumber(String message) {	
		switch( message ) {	
			case "registration_request":
				messageNumber = 0;
			case "registration_response":
				messageNumber = 1;

			case "deregistration_request":
				messageNumber = 2;

			case "deregistration_response":
				messageNumber = 3;

	//		case "messaging_nodes_list":
				messageNumber = 4;

	//		case "link_weights":
				messageNumber = 5;

	//		case "task_initiate":
				messageNumber = 6;

	//		case "task_complete":
				messageNumber = 7;

			 
		}
		return 0;
		
	}
	
}
